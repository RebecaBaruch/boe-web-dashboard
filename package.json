{
  "name": "boe-web-dashboard",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@react-pdf/renderer": "^4.0.0",
    "@svgr/webpack": "^8.1.0",
    "@telefonica/mistica": "16.2.0",
    "@testing-library/dom": "^10.4.0",
    "@types/jspdf": "^2.0.0",
    "@types/node": "^16.18.112",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.0",
    "axios": "^1.7.9",
    "feather-icons-react": "^0.7.0",
    "handlebars": "^4.7.8",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.4",
    "ldrs": "^1.0.2",
    "next": "^15.0.3",
    "react": "latest",
    "react-dom": "latest",
    "react-feather": "^2.0.10",
    "react-loader-spinner": "^6.1.6",
    "react-toastify": "^10.0.6",
    "styled-components": "^6.1.13",
    "typescript": "^4.9.5",
    "uuid": "^11.0.3",
    "uuidv4": "^6.2.13",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:unit": "jest --maxWorkers=50%",
    "test:mutation": "stryker run",
    "test:coverage": "jest --coverage && open coverage/lcov-report/index.html",
    "eject": "react-scripts eject",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "lint-fix": "eslint ./src/**/*.{ts,tsx} --fix",
    "format": "prettier --write \"{src,tests}/**/*.{ts,tsx}\"",
    "prettier": "prettier './src/**/*.{ts,tsx}' --write",
    "generate": "plop --plopfile dev/generators/plopfile.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.12.0",
    "@stryker-mutator/core": "^8.6.0",
    "@stryker-mutator/jest-runner": "^8.6.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.13",
    "@types/mocha": "^10.0.8",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^15.10.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "plop": "^4.0.1",
    "prettier": "^3.3.3",
    "prettier-eslint": "^16.3.0",
    "ts-jest": "^29.2.5",
    "typescript-eslint": "^8.8.0"
  },
  "engines": {
    "node": "18.x"
  },
  "build": {
    "npmCommand": "install"
  }
}
